{
  "name": "track-my-case",
  "version": "1.0.0",
  "description": "[![repo standards badge](https://img.shields.io/endpoint?labelColor=231f20&color=005ea5&style=for-the-badge&label=MoJ%20Compliant&url=https%3A%2F%2Foperations-engineering-reports.cloud-platform.service.justice.gov.uk%2Fapi%2Fv1%2Fcompliant_public_repositories%2Fendpoint%2Ftemplate-repository&logo=data:image/png;base64,)](https://operations-engineering-reports.cloud-platform.service.justice.gov.uk/public-github-repositories.html#template-repository)",
  "main": "index.js",
  "scripts": {
    "prepare": "hmpps-precommit-hooks-prepare",
    "watch-ts": "tsc -w",
    "build": "node esbuild/esbuild.config.js --build",
    "start": "node $NODE_OPTIONS dist/server.js | bunyan -o short",
    "start:locally": "node $NODE_OPTIONS dist/server.js --dev-server | bunyan -o short",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"ESBuild,Node\" -c \"yellow.bold,cyan.bold\" \"node esbuild/esbuild.config.js --build --watch\" \"node esbuild/esbuild.config.js --dev-server\"",
    "start-feature": " node $NODE_DEBUG_OPTION --env-file=.env.production dist/server.js | bunyan -o short",
    "start-feature:dev": "concurrently -k -p \"[{name}]\" -n \"ESBuild,Node\" -c \"yellow.bold,cyan.bold\" \"node --env-file=.env esbuild/esbuild.config.js  --build --watch \" \"node esbuild/esbuild.config.js --dev-server-production\"",
    "dev:victimpathfinder": "tsx watch --clear-screen=false --env-file=.env dist/server.js",
    "lint": "eslint . --cache --max-warnings 0",
    "lint-fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc",
    "test": "jest",
    "test:clearCache": "jest --clearCache",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --runInBand",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run --config video=false",
    "int-test-ui": "cypress open --e2e --browser chrome",
    "clean": "rm -rf dist node_modules",
    "rebuild": "npm run clean && npm i && npm run build",
    "check": "npx npm-check-updates",
    "check:update": "npx npm-check-updates -u",
    "precommit:secrets": "gitleaks git --pre-commit --redact --staged --verbose --config .gitleaks/config.toml",
    "precommit:lint": "node_modules/.bin/lint-staged",
    "precommit:verify": "npm run typecheck && npm test"
  },
  "engines": {
    "node": "^22"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest"
      ]
    },
    "collectCoverageFrom": [
      "server/**/*.{ts,js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/(server|job)/**/?(*.)(cy|test).{ts,js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results/jest/"
        }
      ],
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "test_results/unit-test-reports.html"
        }
      ]
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "node",
      "ts"
    ]
  },
  "lint-staged": {
    "*.{ts,js,css}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^5.1.5",
    "@ministryofjustice/hmpps-auth-clients": "^0.0.1",
    "@ministryofjustice/hmpps-precommit-hooks": "^0.0.3",
    "applicationinsights": "^3.9.0",
    "body-parser": "^2.2.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.8.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "csrf-sync": "^4.2.1",
    "did-resolver": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "govuk-frontend": "^5.11.2",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "jose": "^6.1.0",
    "jwks-rsa": "^3.2.0",
    "jwt-decode": "^4.0.0",
    "nocache": "^4.0.0",
    "nunjucks": "^3.2.4",
    "openid-client": "^5.7.1",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "pino": "^9.9.0",
    "redis": "^5.8.2",
    "sass": "^1.91.0",
    "superagent": "^10.2.3",
    "web-did-resolver": "^2.0.30",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jgoz/esbuild-plugin-typecheck": "^4.0.3",
    "@ministryofjustice/eslint-config-hmpps": "^0.0.3",
    "@tsconfig/node22": "^22.0.2",
    "@types/bunyan": "^1.8.11",
    "@types/bunyan-format": "^0.2.9",
    "@types/compression": "^1.8.1",
    "@types/connect-flash": "0.0.40",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.0",
    "@types/node-fetch": "^2.6.13",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.17",
    "@types/passport-oauth2": "^1.8.0",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "audit-ci": "^7.1.0",
    "aws-sdk-client-mock": "^4.1.0",
    "chokidar": "^3.6.0",
    "concurrently": "^9.2.1",
    "cypress": "^14.5.4",
    "cypress-multi-reporters": "^2.0.5",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "dotenv-expand": "^12.0.2",
    "esbuild": "^0.25.9",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-manifest": "^1.0.5",
    "esbuild-sass-plugin": "^3.3.1",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.5",
    "mocha-junit-reporter": "^2.2.1",
    "nock": "^14.0.10",
    "npm-check-updates": "^18.0.3",
    "pkgroll": "^2.15.3",
    "prettier": "^3.6.2",
    "prettier-plugin-jinja-template": "^2.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "@jgoz/esbuild-plugin-typecheck": {
      "esbuild": "$esbuild"
    },
    "brace-expansion@^1.1.7": "1.1.12",
    "brace-expansion@^2.0.1": "2.0.2"
  }
}
